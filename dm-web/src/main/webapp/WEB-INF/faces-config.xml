<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<navigation-rule>
		<navigation-case>
		 <from-action>#{dashbord.navigateToSearchDonor}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>searchdonor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		 <from-action>#{dashbord.navigateToDashbord}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>dashbord.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		 <from-action>#{dashbord.navigateToSearchCommunicationHistory}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>searchcommunicationhistory.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
		 <from-action>#{dashbord.navigateToExcelUpload}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>donorexcelimport.xhtml</to-view-id>
		</navigation-case>
		
		
		
	</navigation-rule>
	
</faces-config>
