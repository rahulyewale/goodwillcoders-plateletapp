<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<navigation-rule>
		<navigation-case>
		 <from-action>#{dashbord.navigateToSearchDonor}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>secured/searchdonor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		 <from-action>#{dashbord.navigateToSearchDonorOnMap}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>secured/searchdonorwithmap.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		 <from-action>#{dashbord.navigateToDashbord}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>secured/dashboard.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		 <from-action>#{dashbord.navigateToSearchCommunicationHistory}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>secured/searchcommunicationhistory.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
		 <from-action>#{dashbord.navigateToExcelUpload}</from-action>
		 <from-outcome>success</from-outcome>
		 <to-view-id>secured/donorexcelimport.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
		 <from-action>#{dashbord.navigateToRequestList}</from-action>
		 <from-outcome></from-outcome>
		 <to-view-id>secured/requestlist.xhtml</to-view-id>
		</navigation-case>
		
		
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>labels</base-name>
			<var>label</var>
	   </resource-bundle>
     </application>
</faces-config>
